# EMMC
/cust           emmc            /dev/block/bootdevice/by-name/cust		flags=display="Cust";backup=0;flashimg=1
/boot		emmc		/dev/block/bootdevice/by-name/boot
/misc		emmc		/dev/block/bootdevice/by-name/misc		flags=display="Misc";backup=0;flashimg=1
/oeminfo        emmc            /dev/block/bootdevice/by-name/oeminfo		flags=display="OEMinfo";backup=0;flashimg=1
/recovery       emmc            /dev/block/bootdevice/by-name/recovery
/system_image	emmc		/dev/block/bootdevice/by-name/system		flags=backup=0
/vendor_image	emmc		/dev/block/bootdevice/by-name/vendor		flags=backup=0
/version	emmc		/dev/block/bootdevice/by-name/version		flags=display="Version";backup=0;flashimg=1

# Partitions
/cache		ext4		/dev/block/bootdevice/by-name/cache
/data		f2fs		/dev/block/bootdevice/by-name/userdata
/product	ext4		/dev/block/bootdevice/by-name/product		flags=display="Product";backup=0
/system		ext4		/dev/block/bootdevice/by-name/system
/vendor		ext4		/dev/block/bootdevice/by-name/vendor		flags=display="Vendor";backup=1

# USB
/external_sd	vfat		/dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="MicroSD Card";storage;wipeingui;removable
/usbotg		auto		/dev/block/sda1         /dev/block/sda          flags=display="USB OTG";storage;removable;backup=0
